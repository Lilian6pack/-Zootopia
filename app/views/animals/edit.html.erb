<h1 class="text-center">Modifier</h1>
<div class="container">
  <%= simple_form_for @animal do |f| %>
    <%= f.input :name, label: "Nom" %>
    <%= f.input :description %>
    <%= f.input :photo_url, as: :file %>
    <%= f.input :daily_price, label: "Prix journalier" %>
    <%= f.input :address, label: "Adresse" %>
    <%= f.button :submit, "Valider" , class: 'btn-primary' %>
  <% end %>
</div>