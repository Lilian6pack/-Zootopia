<h1 class="text-center">Edit</h1>
<div class="container">
  <%= simple_form_for @animal do |f| %>
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :photo_url, as: :file %>
    <%= f.input :daily_price %>
    <%= f.input :address %>
    <%= f.button :submit, class: 'btn-primary' %>
  <% end %>
</div>